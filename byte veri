using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace byteveri;

internal class byteveri
{
    static void Main(string[] args)
    {
        //Byte : 255'e kadar olan sayıları içerisinde bulundurur.
        byte sayi = 50;
        Console.WriteLine(sayi);
        Console.ReadLine();
    }
}
